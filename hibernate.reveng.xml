<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="photo" catalog="test"
		class="tk.sweetvvck.domain.Photo">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="avatar" catalog="test"
		class="tk.sweetvvck.domain.Avatar">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="banji" catalog="test"
		class="tk.sweetvvck.domain.Banji">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="honor" catalog="test" class="tk.sweetvvck.domain.Honor">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="student" catalog="test" class="tk.sweetvvck.domain.Student">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>